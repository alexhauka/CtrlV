  "bugs": {
    "url": "https://github.com/alexhauka/CtrlV/issues"
  },
  "homepage": "https://github.com/alexhauka/CtrlV#readme",
  "dependencies": {
    "axios": "^0.21.1",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "cheerio": "^1.0.0-rc.5",
    "cookie-session": "^1.4.0",
    "dotenv": "^8.2.0",
    "express": "^4.16.4",
    "express-promise-router": "^4.0.1",
    "heroku": "^7.51.0",
    "nodemon": "^1.18.7",
    "pg": "^8.5.1"
  }
