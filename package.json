{
  "name": "ctrlv",
  "version": "1.0.0",
  "description": "A smart resume builder that generates a resume based off the material you feed it, with pre-defined templates and multiple styling options.",
  "main": "index.js",
  "scripts": {
    "frontend": "cd frontend && npm start",
    "api": "cd backend && nodemon server.js",
    "dev": "concurrently --kill-others-on-fail \"npm run backend\" \"npm run frontend\"",
    "heroku-postbuild": "cd frontend && npm install && npm run build"
  },
